{
    "name": "hexlet/code",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "diglactic/laravel-breadcrumbs": "^7.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "*",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^9",
        "laravel/tinker": "^2.5",
        "laravel/ui": "*",
        "rollbar/rollbar-laravel": "^7.2",
        "spatie/laravel-query-builder": "^5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs app",
        "phpcbf": "phpcbf --standard=PSR12",
        "phpstan": "composer exec --verbose phpstan -- --level=6 analyse app tests",
        "test-coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "composer exec --verbose phpunit tests -- --coverage-clover build/logs/clover.xml"
        ],
        "tc-html": [
            "@putenv XDEBUG_MODE=coverage",
            "composer exec --verbose phpunit tests -- --coverage-html build/logs/html"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
